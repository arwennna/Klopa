<div class="container-lg p-5">

  <div class="row">
    <div class="col-lg-8">
      <div id="map" style="width: 100%; height: 530px;"></div>
    </div>
    <div class="col-lg-4">

      <form id="forma_adresa" method="post" action="/mojprofil">
        <fieldset>
          <legend>Molimo, unesite podatke o Va≈°oj adresi.</legend>

          <div class="form-group">
            <label for="ulica">
              Ulica
            </label>
            <input type="text" class="form-control" id="ulica" name="ulica" required >
            <small id="fileHelp" class="form-text text-muted">Navedite ispravno ime ulice i broj.</small>
          </div>

          <div class="row">
            <div class="col">
              <input type="text" class="form-control" placeholder="Latituda" id="latituda" name="latituda" readonly="readonly" required>
            </div>
            <div class="col">
              <input type="text" class="form-control" placeholder="Longituda" id="longituda" name="longituda" readonly="readonly" required>
            </div>
          </div>
          <br>
          <button type="submit" class="btn btn-primary" id="dugmic">Spasi</button>
        </fieldset>
      </form>
    </div>
  </div>
</div>

